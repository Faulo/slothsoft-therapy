<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">		<xsl:template match="/data">		<xsl:apply-templates select="*[@name='happiness']" mode="graph"/>	</xsl:template>		<xsl:template match="*" mode="graph">		<xsl:variable name="ratingList" select="line/cell[1]/@val"/>				<graph scaleX="5" scaleY="20" padding-left="200" padding-top="20" padding-bottom="20">			<style><![CDATA[.graph {	display: block;	background: silver;}.graph text {	font-weight: bold;	font-size: 1.3em;	fill: white;	stroke: black;	font-family: sans-serif;}.graph .graph-axes path,.graph .graph-values rect {	stroke: black;}			]]></style>			<xAxis min="0" max="365" size="10">				<xsl:for-each select="$ratingList">					<xsl:variable name="date" select="../following-sibling::cell/@val"/>					<xsl:if test="position() mod 14 = 1 and string-length($date)">						<label position="{position() - 0.5}"><xsl:value-of select="$date"/></label>					</xsl:if>				</xsl:for-each>			</xAxis>			<yAxis min="-10" max="10" size="8">				<label position="-10">-10</label>				<label position="-2.5">(unzufrieden) -2,5</label>				<label position="-5">(ungl端cklich) -5</label>				<label position="-7.5">(todtraurig) -7,5</label>				<label position="2.5">(zufrieden) 2,5</label>				<label position="5">(gl端cklich) 5</label>				<label position="7.5">(端bergl端cklich) 7,5</label>				<label position="10">10</label>			</yAxis>			<valueList>				<xsl:for-each select="$ratingList">					<xsl:variable name="red"><xsl:value-of select="round(127 - 12.8 * .)"/></xsl:variable>					<xsl:variable name="green"><xsl:value-of select="round(127 + 12.8 * .)"/></xsl:variable>					<value x="{position() - 1}" y="{.}" style="fill: rgb({$red}, {$green}, 0)"/>				</xsl:for-each>			</valueList>		</graph>	</xsl:template></xsl:stylesheet>